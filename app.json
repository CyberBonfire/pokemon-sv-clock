{
  "expo": {
    "name": "Pokémon SV clock",
    "slug": "pokemon-sv-clock",
    "version": "0.0.1",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "pokemon-sv-clock",
    "owner": "cymagix",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#8B021A",
    "primaryColor": "#6b32b9",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#8B021A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.pokemon-sv-clock",
      "buildNumber": "0.0.1",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      "package": "com.pokemon_sv_clock",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#8B021A"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "name": "Pokémon SV clock"
    },
    "locales": {
      "en": "./lib/i18n/english.json",
      "ja": "./lib/i18n/japanese.json"
    },
    "plugins": [],
    "extra": {
      "eas": {
        "projectId": "f1258bf3-af9f-4c8f-90cc-ef437bc877ca"
      }
    }
  }
}
